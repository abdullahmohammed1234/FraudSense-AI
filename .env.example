# =============================================================================
# FraudSense AI - Environment Configuration
# =============================================================================
# Copy this file to .env and configure for your environment

# API Configuration
API_PORT=8001
LOG_LEVEL=INFO

# API Keys (format: key:role:rate_limit)
# Roles: Admin, Analyst, Auditor
# Rate limit: requests per minute
API_KEYS=dev-key-001:Admin:1000,analyst-key-001:Analyst:500,auditor-key-001:Auditor:200

# Docker Configuration
NGINX_PORT=80
NGINX_SSL_PORT=443

# Database (for future use)
# DATABASE_URL=postgresql://user:pass@localhost:5432/fraudsense

# Redis (for future use)
# REDIS_URL=redis://localhost:6379/0

# Model Configuration
MODEL_VERSION=1.0.0
BLOCK_THRESHOLD=0.75
REVIEW_THRESHOLD=0.40
